{
    "name": "payum/klarna-invoice",
    "type": "library",
    "description": "Klarna Invoice",
    "keywords": [
        "payment",
        "klarna"
    ],
    "homepage": "http://payum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "http://payum.org/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/KlarnaInvoice/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "payum/core": "0.11.*@dev",
        "klarna/invoice": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Klarna\\Invoice": "" },
        "classmap": ["klarna-invoice"]
    },
    "target-dir": "Payum/Klarna/Invoice",
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    },
    "repositories": {
        "klarna/invoice": {
            "type": "vcs",
            "url": "git@github.com:formapro/KlarnaInvoice.git"
        }
    }
